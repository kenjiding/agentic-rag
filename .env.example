# OpenAI API 配置
OPENAI_API_KEY=your_openai_api_key_here

# LLM 配置
MODEL_NAME=gpt-3.5-turbo
TEMPERATURE=0.7
MAX_TOKENS=

# 嵌入模型配置
EMBEDDING_MODEL=text-embedding-3-small

# Agentic RAG 配置
MAX_ITERATIONS=5
RETRIEVAL_QUALITY_THRESHOLD=0.7
ANSWER_QUALITY_THRESHOLD=0.7

# 向量数据库配置
PERSIST_DIRECTORY=./data/chroma_db/agentic_rag

# 其他配置
VERBOSE=true
ENABLE_CACHING=true
